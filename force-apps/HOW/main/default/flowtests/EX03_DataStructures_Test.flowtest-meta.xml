<?xml version="1.0" encoding="UTF-8"?>
<FlowTest xmlns="http://soap.sforce.com/2006/04/metadata">
    <flowApiName>EX03_DataStructures</flowApiName>
    <label>EX03_DataStructures_Test</label>
    <testPoints>
        <elementApiName>Start</elementApiName>
        <parameters>
            <leftValueReference>$Record</leftValueReference>
            <type>InputTriggeringRecordInitial</type>
            <value>
                <sobjectValue>{&quot;FirstName&quot;:&quot;Andrés&quot;,&quot;LastName&quot;:&quot;Pérez&quot;}</sobjectValue>
            </value>
        </parameters>
    </testPoints>
    <testPoints>
        <assertions>
            <conditions>
                <leftValueReference>results.current.LastName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pérez</stringValue>
                </rightValue>
            </conditions>
        </assertions>
        <assertions>
            <conditions>
                <leftValueReference>results.current.FirstName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Andrés</stringValue>
                </rightValue>
            </conditions>
        </assertions>
        <elementApiName>Finish</elementApiName>
    </testPoints>
</FlowTest>
