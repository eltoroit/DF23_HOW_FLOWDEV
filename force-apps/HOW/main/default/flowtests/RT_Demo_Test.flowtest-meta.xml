<?xml version="1.0" encoding="UTF-8"?>
<FlowTest xmlns="http://soap.sforce.com/2006/04/metadata">
    <flowApiName>RT_Demo</flowApiName>
    <label>RT_Demo_Test</label>
    <testPoints>
        <elementApiName>Start</elementApiName>
        <parameters>
            <leftValueReference>$Record</leftValueReference>
            <type>InputTriggeringRecordInitial</type>
            <value>
                <sobjectValue>{&quot;FirstName&quot;:&quot;Andres&quot;,&quot;LastName&quot;:&quot;Perez&quot;}</sobjectValue>
            </value>
        </parameters>
    </testPoints>
    <testPoints>
        <assertions>
            <conditions>
                <leftValueReference>Apex010.outputContact.myContact.LastName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Perez</stringValue>
                </rightValue>
            </conditions>
        </assertions>
        <assertions>
            <conditions>
                <leftValueReference>Apex010.outputContact.myContact.FirstName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Andres</stringValue>
                </rightValue>
            </conditions>
        </assertions>
        <elementApiName>Finish</elementApiName>
    </testPoints>
</FlowTest>
